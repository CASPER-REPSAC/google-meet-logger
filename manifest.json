{
    "name": "google-meet-logger",
    "description": "google meet logger extension",
    "version": "0.1",
    "manifest_version": 3,
    "options_page": "options.html",
    "content_scripts" : [
      {
        "matches": ["https://meet.google.com/*"],
        "js" : ["/contents/main.js"]
      }
    ],
    "permissions" : ["activeTab", "storage"]
}